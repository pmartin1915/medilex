{
  "expo": {
    "name": "Healthcare Vocab",
    "slug": "healthcare-vocab",
    "description": "Master medical terminology with interactive flashcards, streak tracking, and comprehensive progress analytics. 100% offline, zero data collection.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.20"
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.medilex.healthcarevocab",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app uses your device's text-to-speech for audio pronunciation of medical terms. No audio is recorded or transmitted."
      }
    },
    "android": {
      "package": "com.medilex.healthcarevocab",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ee690d6d-a26d-4b2f-a820-64e4aa1b5b21"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "ee690d6d-a26d-4b2f-a820-64e4aa1b5b21"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
